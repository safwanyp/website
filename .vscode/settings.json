{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "prettier.documentSelectors": [
    "**/*.astro",
    "**/*.(c|m)js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx"
  ],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
